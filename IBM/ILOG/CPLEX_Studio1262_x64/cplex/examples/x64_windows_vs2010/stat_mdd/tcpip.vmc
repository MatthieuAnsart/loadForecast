<!-- Example VMC file for two workers to which the master
     connects via TCP/IP. The configuration uses a fixed port on the localhost.
     The workers configured here must be started explicitly. Adapt as needed.
-->
<vmc>
  <machine name="machine1">
    <transport type="TCP/IP">
      <address host="localhost" port="12345" />
    </transport>
  </machine>
  <machine name="machine2">
    <transport type="TCP/IP">
      <address host="localhost" port="12345" />
    </transport>
  </machine>
</vmc>
